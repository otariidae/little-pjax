{
  "name": "little-pjax",
  "description": "A simple, small and modern implementation of pjax",
  "version": "0.3.1",
  "author": "otariidae",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "license": "CC0-1.0",
  "homepage": "https://github.com/otariidae/pjax",
  "repository": {
    "type": "git",
    "url": "https://github.com/otariidae/pjax.git"
  },
  "bugs": {
    "url": "https://github.com/otariidae/pjax/issues"
  },
  "scripts": {
    "prelint": "prettier --write src/*.js test/*.js",
    "lint": "eslint --fix src/*.js",
    "check": "flow check",
    "pretest": "npm run build:cjs",
    "test": "kocha test/*.js",
    "prebuild": "rm -rf dist && mkdir dist",
    "build": "run-p build:*",
    "build:cjs": "cross-env NODE_ENV=cjs babel src --out-dir dist/cjs",
    "build:esm": "babel src --out-dir dist/esm",
    "build:flow": "flow gen-flow-files src --out-dir dist"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-modules-commonjs": "^7.5.0",
    "@babel/preset-flow": "^7.0.0",
    "babel-eslint": "^10.0.2",
    "cross-env": "^5.1.3",
    "eslint": "^6.1.0",
    "flow-bin": "^0.105.0",
    "jsdom": "^15.1.1",
    "kocha": "^1.9.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2"
  }
}
